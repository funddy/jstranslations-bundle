{
    "name": "funddy/jstranslations-bundle",
    "description": "JsTranslations for Symfony2",
    "keywords": ["js translations", "translations", "translator"],
    "homepage": "https://github.com/funddy/jstranslations-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Keyvan Akbary",
            "email": "keyvan@funddy.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.6",
        "symfony/serializer": ">=2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.7.2"
    },
    "autoload": {
        "psr-0": { "Funddy\\Bundle\\JsTranslationsBundle": "" }
    },
    "target-dir": "Funddy/Bundle/JsTranslationsBundle",
    "config": {
        "bin-dir": "bin"
    }
}
